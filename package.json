{
  "name": "mergejsfiles",
  "version": "1.0.0",
  "description": "合并 js 文件工具，本工具根据输入路径，扫描路径下的所有js文件，压缩并合成一个文件，根据内容计算哈希值，重命名。",
  "main": "index.js",
  "scripts": {
    "mjsf": "bin/index.js --mjsf example/config/MJSFOptions.json"
  },
  "keywords": [
    "tool",
    "mergejsfiles"
  ],
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "bin": {
    "mergejsfiles": "bin/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "glob": "^10.4.3",
    "terser": "^5.31.1",
    "yargs": "^17.7.2"
  }
}
