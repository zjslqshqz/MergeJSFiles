{
  "options": {
    "output": {
      "dir": ["example","dist"],
      "prefix": "main",
      "suffix": "min.js",
      "writeBootFile": true,
      "bootFileAddress": ["example","dist","index.php"],
      "scriptTagSrcPrefixDir": ["/","example"]
    },
    "maxFileSize": 102400
  },
  "globOptions": {
    "pattern": ["js/**/*.js",["ext","**","*.js"]],
    "options": {
      "cwd": ["example"],
      "ignore": "",
      "nodir": true
    }
  },
  "minifyOptions": {
    "files": "",
    "options": {
      "compress": true,
      "mangle": false,
      "output": {
        "comments": false,
        "ascii_only": true
      }
    }
  }
}
